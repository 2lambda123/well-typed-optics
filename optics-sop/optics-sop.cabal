name: optics-sop
version: 0.1
license: BSD3
license-file: LICENSE
build-type: Simple
cabal-version: >= 1.10
tested-with: GHC == 8.0.2, GHC == 8.2.2, GHC == 8.4.4, GHC == 8.6.3

library
  default-language: Haskell2010
  hs-source-dirs: src
  ghc-options: -Wall
  build-depends:
    base >= 4.9 && < 5,
    generics-sop >= 0.3.1.0 && < 0.4,
    optics-core >= 0.1 && < 1
  exposed-modules:
    Generics.SOP.Optics
    Optics.SOP
    Optics.SOP.ToTuple
