name: optics-sop
version: 0.1
license: BSD3
license-file: LICENSE
build-type: Simple
cabal-version: >= 1.10
tested-with: GHC == 7.10.3, GHC == 8.0.2

library
  default-language: Haskell2010
  hs-source-dirs: src
  ghc-options: -Wall
  build-depends:
    base >= 4.8 && < 5,
    generics-sop >= 0.2.4.0 && < 0.4,
    optics-core >= 0.1 && < 1
  exposed-modules:
    Generics.SOP.Optics
    Optics.SOP
    Optics.SOP.ToTuple
